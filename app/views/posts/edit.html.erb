<turbo-frame id="<%= dom_id(@post)%>" class="post">
    <div class="form">
        <h1> Edit post </h1>
        <%= form_with(model: @post, local: true) do |f| %>
            <%= f.label :description %>
            <%= f.text_area :body %>
            <%= f.file_field :image %>
            <%= f.submit%>
            <%= link_to "close", root_path%>
        <% end %>
    </div>
</turbo-frame>